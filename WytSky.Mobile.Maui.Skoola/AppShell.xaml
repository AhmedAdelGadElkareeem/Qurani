<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="WytSky.Mobile.Maui.Skoola.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:language="clr-namespace:WytSky.Mobile.Maui.Skoola.Views.Public"
    xmlns:local="clr-namespace:WytSky.Mobile.Maui.Skoola"
    xmlns:views="clr-namespace:WytSky.Mobile.Maui.Skoola.Views.Quarni"
    Shell.FlyoutBehavior="Flyout">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="Center"
        ContentTemplate="{DataTemplate views:CenterPage}"
        Route="CenterPage" />


</Shell>
